<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#F4F4F4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TableLayout android:layout_width="wrap_content"
        android:layout_height="36.585pt"
        android:stretchColumns="0"
        android:background="#F4F4F4">
        <TableRow>
            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="79.695pt"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5.715pt"
                android:background="#F4F4F4"
                >
                <com.asana.hbl.views.CircularImageView
                    android:id="@+id/image_player"
                    android:layout_width="24.615pt"
                    android:layout_height="24.615pt"
                    android:layout_marginTop="6.3pt"
                    />
                <TextView
                    android:id="@+id/tv1"
                    android:text="GG"
                    android:layout_width="21.915pt"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30.42pt"
                    android:layout_marginTop="16.785pt"
                    android:textSize="6.885pt"
                    android:textColor="@android:color/black"
                    />
            </RelativeLayout>
            <TableLayout
                android:layout_width="85.185pt"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:layout_marginRight="19.35pt"
                android:layout_marginTop="16.965pt"
                android:background="#F4F4F4"
                >
                <TableRow
                    android:layout_marginLeft="3.6pt">
                    <TextView
                        android:id="@+id/tv2"
                        android:background="#F4F4F4"
                        android:text="l1"
                        android:textColor="@android:color/black"
                        android:textSize="5.08pt"
                        android:textStyle="bold"
                        android:layout_margin="2dip"
                        />
                    <TextView
                        android:id="@+id/tv3"
                        android:background="#F4F4F4"
                        android:text="l2"
                        android:textColor="@android:color/black"
                        android:textSize="5.08pt"
                        android:textStyle="bold"
                        android:layout_margin="2dip"/>
                    <TextView
                        android:id="@+id/tv4"
                        android:background="#F4F4F4"
                        android:text="l3"
                        android:textColor="@android:color/black"
                        android:textSize="5.08pt"
                        android:textStyle="bold"
                        android:layout_margin="2dip"/>
                    <TextView
                        android:id="@+id/tv5"
                        android:background="#F4F4F4"
                        android:textColor="@android:color/black"
                        android:text="l4"
                        android:textSize="5.08pt"
                        android:textStyle="bold"
                        android:layout_margin="2dip"/>
                </TableRow>
            </TableLayout>
        </TableRow>
    </TableLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19.5pt"
        android:layout_marginLeft="171pt"
        >
        <ImageView
            android:id="@+id/nav_arrow"
            android:layout_width="6.615pt"
            android:layout_height="3.285pt"
            android:background="@mipmap/angle_arrow_down"/>
    </RelativeLayout>
</RelativeLayout>